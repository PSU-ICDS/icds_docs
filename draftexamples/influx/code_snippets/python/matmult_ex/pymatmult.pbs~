#!/bin/bash
#PBS -A open
#PBS -l nodes=1:ppn=1
#PBS -l walltime=01:00:00

# Get started
echo " "
echo "Job started on `hostname` at `date`"
echo " "

#load in python
module purge
module load python

#go to the correct work directory
cd $PBS_O_WORKDIR

python pymatmult.py < dim_file.txt > log.matmult

# Finish up
echo " "
echo "Job Ended at `date`"
echo " "
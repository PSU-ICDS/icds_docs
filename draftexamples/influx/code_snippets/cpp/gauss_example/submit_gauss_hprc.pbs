#!/bin/bash

#PBS -N gauss_demo
#PBS -A ics-hprc
#PBS -q hprc
#PBS -l nodes=1:ppn=1
#PBS -l pmem=4gb
#PBS -l walltime=5:00

echo "Job started on `hostname` at `date`"

# Load software
module purge
module load gcc

# Go to the correct place
cd $PBS_O_WORKDIR

# Run the job itself - a matlab script called runThis.m
g++ gauss.cpp -o gauss
./gauss

# Finish up
echo "Job Ended at `date`"

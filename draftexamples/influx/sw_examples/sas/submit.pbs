# Get started
echo " "
echo "Job started on `hostname` at `date`"
echo " "

# Load in module 
module load sas 
echo "SAS is loaded"
# Go to the correct place
cd $PBS_O_WORKDIR
echo "loaded to the pwd"
# Run the job
sas sample.sas -log output.log

# Finish up
echo " "
echo "Job Ended at `date`"
echo " "

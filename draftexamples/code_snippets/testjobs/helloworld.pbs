#!/bin/bash

#PBS -N helloworld
#PBS -A open
#PBS -l nodes=1:ppn=1
#PBS -l pmem=1gb
#PBS -l walltime=5:00

# Get started
echo " "
echo "Job started on `hostname` at `date`"
echo " "

# Go to the submission directory
cd $PBS_O_WORKDIR

# Run the main job commands
module load python
echo "Job is now running..."
python helloworld.py -o hw_output.txt

# Finish up
echo " "
echo "Job Ended at `date`"
#!/bin/bash
#PBS -l nodes=1:ppn=1
#PBS -l walltime=5:00
#PBS -l pmem=1gb
#PBS -A open

# Get started
echo " "
echo "Job started on `hostname` at `date`"
echo " "

# Load in matlab 
module purge
module load matlab

# Go to the correct place
cd $PBS_O_WORKDIR

# Run the job itself
matlab-bin -nodisplay -nosplash  -singleCompThread  < matmult.m > log.matmult

# Finish up
echo " "
echo "Job Ended at `date`"
echo " "


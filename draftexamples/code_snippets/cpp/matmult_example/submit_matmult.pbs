#!/bin/bash

#PBS -N matmult_job
#PBS -A open
#PBS -l nodes=1:ppn=1
#PBS -l pmem=4gb
#PBS -l walltime=5:00

echo "Job started on `hostname` at `date`"

# Load software
module purge
module load gcc

# Go to the correct place
cd $PBS_O_WORKDIR

# Compile and run
g++ matmult.cpp -o matmult
./matmult 1000 1200 1000

# Finish up
echo "Job Ended at `date`"
